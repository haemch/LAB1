<?xml version="1.0" encoding="UTF-8"?>
<stl:ProductionLineModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:stl="http://www.example.org/stl"
    xsi:schemaLocation="http://www.example.org/stl stl.ecore">
  <itemtypes
      name="IT1"/>
  <itemtypes
      name="IT2"/>
  <area components="//@components.1"
      connectors="//@connectors.0"/>
  <area name="a2"
      components="//@components.0"/>
  <connectors
      area="//@area.0"
      inputslot="//@slots.0"
      outputslot="//@slots.1"
      input_component="//@components.1"
      output_component="//@components.0"/>
  <components
      xsi:type="stl:ProductStore"
      area="//@area.1"
      costs="3"
      capacity="2"
      inputslot="//@slots.0"
      itemtypes="//@itemtypes.0 //@itemtypes.1"/>
  <components
      xsi:type="stl:ItemGenerator"
      area="//@area.0"
      name=""
      outputslot="//@slots.1"
      itemtypes="//@itemtypes.1"/>
  <slots xsi:type="stl:InputSlot"
      requiredAsImput="//@itemtypes.1"
      connectors="//@connectors.0"/>
  <slots xsi:type="stl:OutputSlot"
      name=""
      producedAsOutput="//@itemtypes.1"
      connectors="//@connectors.0"/>
</stl:ProductionLineModel>
