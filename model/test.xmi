<?xml version="1.0" encoding="UTF-8"?>
<stl:ProductionLineModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:stl="http://www.example.org/stl"
    xsi:schemaLocation="http://www.example.org/stl stl.ecore">
  <itemtypes
      name="IT1"/>
  <itemtypes
      name="IT2"/>
  <area components="//@components.0 //@components.1"/>
  <components
      xsi:type="stl:ProductStore"
      area="//@area.0"
      costs="3"
      capacity="2"
      inputslot="//@slots.0"
      itemtypes="//@itemtypes.0 //@itemtypes.1">
    <service
        reliability="0.4"/>
  </components>
  <components
      xsi:type="stl:ItemGenerator"
      area="//@area.0"
      name=""
      outputslot="//@slots.1"
      itemtypes="//@itemtypes.1"/>
  <slots xsi:type="stl:InputSlot"
      requiredAsImput="//@itemtypes.0"/>
  <slots xsi:type="stl:OutputSlot"
      producedAsOutput="//@itemtypes.1"/>
</stl:ProductionLineModel>
