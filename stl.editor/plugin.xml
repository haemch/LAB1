<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated stl -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="stl.presentation.StlModelWizardID"
            name="%_UI_StlModelWizard_label"
            class="stl.presentation.StlModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/StlModelFile.gif">
         <description>%_UI_StlModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated stl -->
      <editor
            id="stl.presentation.StlEditorID"
            name="%_UI_StlEditor_label"
            icon="icons/full/obj16/StlModelFile.gif"
            extensions="stl"
            class="stl.presentation.StlEditor"
            contributorClass="stl.presentation.StlActionBarContributor">
      </editor>
   </extension>

</plugin>
